{
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/api/$1"
    },
    {
      "src": "^/admin/(.*)",
      "dest": "/admin/index.html"
    },
    {
      "src": "^/inscription/(.*)",
      "dest": "/inscription/index.html"
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ]
}
