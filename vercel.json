{
  "version": 2,
  "rewrites": [
    {
      "source": "/api/(.*)",
      "destination": "/api/$1"
    },
    {
      "source": "^/admin/(.*)",
      "destination": "/admin/index.html"
    },
    {
      "source": "^/inscription/(.*)",
      "destination": "/inscription/index.html"
    },
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ],
 "builds": [
    {
      "src": "index.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/index.js"
    }
  ]
}
